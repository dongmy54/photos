<h1>上传图片</h1>

<div class="row">
	<div class="col-md-4 col-md-offset-4">
		<%= form_for @photo do |f| %>
			<%= render "common/error_messages" %>

			<%= f.label :title %>
			<%= f.text_field :title, class: "form-control" %>
      
      <%= f.label :tag %>
      <%= f.text_field :tag , class: "form-control" %>

      <%= f.label :image %>
      <%= f.file_field :image %><br>

      <%= f.submit "Submit", data: { disable_with: "Submitting..." }, class: "btn btn-block btn-info" %>


    <% end %>
	</div>
</div>