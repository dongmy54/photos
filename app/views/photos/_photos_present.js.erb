draggable_opts = {
        helper: "clone",
        revert: true,
        opacity: 0.4,
        start: function(evt,ui) {
          $(this).addClass("drag-and-drog");
        },
        stop: function(evt,ui) {
          $(this).removeClass("drag-and-drog");
        }
       
    };

<% @photos.each do |photo| %>
  image = $('<%= image_tag(photo.image.thumb.url, class: "img-rounded photo-source",  data: { :url => photo_path(photo)} ) %>').draggable(draggable_opts);
  $(".photos-list").append( '<div  class="col-md-4" id="photo-<%= photo.id %>"></div>');
	$(".photos-list .col-md-4").last().append(image);
	$(".photos-list .col-md-4").last().append('<p><strong>Title:</strong> <%= photo.title %></p><p><strong>Date:</strong>  <%= photo.updated_at.strftime("%Y/%m/%d") %></p><p><strong>Tag:</strong>  <%= photo.tag %></p></div>');
<% end %>

$(".photos-list #title").html('photos in <%= @photos.first.tag %>');

$(".paginate").html('<%=j render partial: "paginate", locals: {:photos => @photos} %>');